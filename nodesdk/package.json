{
  "name": "@gnaudio/jabra-node-sdk",
  "version": "3.2.1-beta.5",
  "description": "NodeJS SDK for Jabra Devices",
  "main": "dist/main/index.js",
  "types": "dist/main/index.d.ts",
  "keywords": [
    "jabra",
    "sdk"
  ],
  "files": [
    "dist/doc",
    "dist/main",
    "dist/api-meta.json",
    "libjabra",
    "includes",
    "src/main",
    "binding.gyp",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:dev",
    "build:dev": "node-gyp rebuild --debug && npm run tsc && npm run generatemeta",
    "build:release": "node-gyp rebuild && npm run tsc && npm run generatemeta",
    "tsc": "tsc",
    "prepare": "npm run tsc && npm run doc && node dist/script/generatemeta.js",
    "generatemeta": "ts-node src/script/generatemeta.ts",
    "manualtest": "cross-env LIBJABRA_TRACE_LEVEL=trace ts-node src/manualtest/misc.ts",
    "example-btn-press-ts": "cross-env LIBJABRA_TRACE_LEVEL=trace ts-node src/examples/button-press.ts",
    "example-btn-press-js": "cross-env LIBJABRA_TRACE_LEVEL=trace ts-node src/examples/button-press.js",
    "example-ringer-sequence": "cross-env LIBJABRA_TRACE_LEVEL=trace ts-node src/examples/ringer-sequence.ts",
    "doc": "typedoc --mode file --excludePrivate --excludeExternals --excludeNotExported --out dist/doc src/main",
    "clean": "node-gyp clean && rimraf dist",
    "pipeline-build-setversion": "node setversion.js"
  },
  "author": "GN Audio",
  "license": "MIT",
  "gypfile": true,
  "homepage": "https://github.com/gnaudio/jabra-node-sdk/",
  "repository": {
    "type": "git",
    "url": "https://github.com/gnaudio/jabra-node-sdk.git",
    "directory": "nodesdk"
  },
  "bugs": {
    "url": "https://developer.jabra.com"
  },
  "devEngines": {
    "node": ">=10.0.0",
    "npm": ">=5.5.1"
  },
  "dependencies": {
    "bindings": "1.5.0",
    "node-addon-api": "1.7.1"
  },
  "devDependencies": {
    "@types/bindings": "1.3.0",
    "@types/node": "13.11.1",
    "cross-env": "7.0.2",
    "replace-in-file": "5.0.2",
    "rimraf": "3.0.2",
    "ts-node": "8.8.2",
    "typedoc": "0.17.4",
    "typescript": "3.8.3"
  }
}
